{
  "name": "infi-canvas",
  "version": "0.1.0",
  "private": true,
  "productName": "InfiCanvas",
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@electron/remote": "^2.0.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/recoil": "^0.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "recoil": "^0.5.2",
    "typescript": "^4.4.4",
    "electron-is-dev": "^2.0.0",
    "web-vitals": "^1.1.2"
  },
  "main": "build/electron.js",
  "scripts": {
    "start": "craco start",
    "build:react": "craco build",
    "build:electron": "electron-packager . --overwrite --icon=build/osx/icon.icns",
    "build": "npm run build:react && npm run build:electron",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "electron": "^15.3.1",
    "electron-packager": "^15.4.0",
    "wait-on": "^6.0.0"
  }
}
